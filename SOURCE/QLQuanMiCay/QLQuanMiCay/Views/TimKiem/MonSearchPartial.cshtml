@model IPagedList<BLL_DAL.Mon>
@using PagedList
@using PagedList.Mvc

@{
    if (Model.Count == 0)
    {
        <h3>Không có kết quả phù hợp</h3>
    }
    else
    {
        <div class="col-sm-12" id="monPartialZone">
            @Html.Partial("~/Views/Mon/MonPartial.cshtml", Model)
        </div>
    }
}
